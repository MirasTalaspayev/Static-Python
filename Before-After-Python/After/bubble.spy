def bubbleSort(arr: list[int]) -> None:
    n: int = len(arr)
    for i:int in range(n):
        for j:int in range(0, n-i-1):
            if arr.get(j) > arr.get(j+1):
                temp: int = arr.get(j)
                arr.put(j, arr.get(j+1))
                arr.put(j+1, temp)

arr: list[int] = [5, 1, 4, 2, 8]
bubbleSort(arr)

print("Sorted array is:")
for i:int in range(len(arr)):
    print(arr.get(i) )
