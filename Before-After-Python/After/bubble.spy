def bubbleSort(arr: list[int]):
    n: int = len(arr)
    for i:int in range(n):
        for j:int in range(0, n-i-1):
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]
 
 
if __name__ == "__main__":
  arr: list[int] = [5, 1, 4, 2, 8]
 
  bubbleSort(arr)
 
  print("Sorted array is:")
  for i:int in range(5):
      print("%d" % arr[i], end=" ")
